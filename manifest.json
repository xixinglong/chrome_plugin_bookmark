{
    "manifest_version": 3,
    "name": "书签管理器",
    "version": "1.0",
    "description": "一个强大的Chrome书签管理与搜索工具",
    "permissions": [
        "bookmarks",
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}